{
  "name": "note-taking",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --no-progress --host 0.0.0.0",
    "start-hmr": "ng serve --no-progress --configuration hmr --host 0.0.0.0",
    "prod": "ng build --prod --progress=false",
    "build": "ng build",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "pretest": "yarn clean",
    "test": "ng test",
    "test-watch": "yarn test --watch",
    "coverage": "yarn test --coverage",
    "clean": "rimraf dist && rimraf out-tsc && rimraf coverage",
    "clean-build": "yarn lint && yarn test && yarn prod && yarn clean",
    "bundle-report": "yarn clean && yarn prod --stats-json && yarn webpack-bundle-analyzer dist/stats.json",
    "http-server": "angular-http-server --path ./dist",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.5",
    "@angular/cdk": "8.2.3",
    "@angular/common": "~8.2.5",
    "@angular/compiler": "~8.2.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~8.2.5",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.5",
    "@angular/platform-browser-dynamic": "~8.2.5",
    "@angular/router": "~8.2.5",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.4",
    "@angular/cli": "~8.3.4",
    "@angular/compiler-cli": "~8.2.5",
    "@angular/language-service": "~8.2.5",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "husky": "4.x",
    "jest": "25.x",
    "jest-junit": "10.x",
    "jest-preset-angular": "8.x",
    "prettier": "2.x",
    "rimraf": "3.x",
    "ts-jest": "25.x",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
